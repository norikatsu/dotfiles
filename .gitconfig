[core]
	quotepath = false
[user]
	name = Yoshida Norikatsu
	email = norikatsu@gmail.com
[color]
	ui = auto
[diff]
	tool = gvimdiff
[difftool]
	prompt = false
[alias]
	co = checkout
	br = branch
	ci = commit -a
	st = status
	svdc = svn dcommit
	difh = diff HEAD
	diffh = diff HEAD
	svrb = svn rebase
	hint = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short

